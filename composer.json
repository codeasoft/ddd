{
  "name": "termyn/ddd",
  "description": "A set of design patterns and building blocks that you can use to design domain-driven system in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "autoload": {
    "psr-4": {
      "Termyn\\Ddd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Termyn\\Ddd\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "termyn/date-time": "^0.1",
    "termyn/event": "^0.1"
  },
  "require-dev": {
    "phpstan/phpstan": "1.10.*",
    "phpstan/phpstan-phpunit": "1.3.*",
    "phpunit/phpunit": "10.2.*",
    "symplify/easy-coding-standard": "11.5.*"
  }
}
